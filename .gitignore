# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore local settings (can include sensitive info like database credentials).
/web/sites/*/settings.local.php
/drush/local/drush.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore files automatically generated by Drupal.
/config/README.txt
/config/splits/*/.htaccess
/config/sync/.htaccess
/config/autocommit/.htaccess
/config/autocommit/*

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by IDEs.
/.buildpath
/.idea/
/.vscode/
/.project
/.settings
/*.esproj
/*.komodoproject
/*.komodotools
/nbproject

# Ignore files generated by Docker.
/docker-runtime
/mariadb-init
/.docker-sync

# Ignore OS specific temporary files.
.DS_Store*
ehthumbs.db
Thumbs.db

# Ignore database dumps and archives.
*.sql
*.gz
*.xz

# Ignore files generated by build tools.
.sass-cache
node_modules
npm-debug.log
/web/kalastatic

# Ignore files built out by Drupal Scaffold.
/web/.csslintrc
/web/.editorconfig
/web/.eslintignore
/web/.eslintrc
/web/.eslintrc.json
/web/.gitattributes
/web/.htaccess
/web/.ht.router.php
/web/autoload.php
/web/index.php
/web/modules/.gitkeep
/web/profiles/.gitkeep
/web/robots.txt
/web/sites/default/default.services.yml
/web/sites/default/default.settings.php
/web/sites/example.settings.local.php
/web/sites/example.sites.php
/web/themes/.gitkeep
/web/update.php
/web/web.config

# Ignore Drupal VM files.
/.vagrant
/local.config.yml

# Ignore private files (though the folder must pre-exist).
/private

# Ignore .env files as they are personal
/.env
